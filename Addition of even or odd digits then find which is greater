import java.util.*;

public class index {
public static void main(String[] args) {
    int num=198;
    int even=0;
    int odd=0;


  while(num!=0){
     int digit= num%10;
     if(digit%2==0)
        even=even+digit;
    else
        odd=odd+digit;

       num=num/10;

    }

  if(odd>even)
     System.out.print(odd);
  else
    System.out.print(even);




}
}

