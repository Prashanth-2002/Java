class HelloWorld {
    public static void main(String[] args) {
       int a =500;
       int b=71;
       int c=20;
       
       if(a>b && a>c)
       System.out.println("a");
      else  if(b>a && b>c)
       System.out.println("b");
      else 
      System.out.println("c");
    }
}
