class HelloWorld {
    public static void main(String[] args) {
       int num =12;
       boolean prime=true;
     
      
      if(num<2){
         prime=false;
      }
     else  {
      for(int i=2;i<num;i++){
          if(num%i==0){
           prime=false;
           break;
          }
           
      }     
         
     } 
     
     if(prime)
     System.out.print("Yes");
     else
       System.out.print("NO");
     
      
      
        
    }
}
